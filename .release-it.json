{
    "dry-run": false,
    "requireCleanWorkingDir": true,
    "non-interactive": false,
    "verbose": false,
    "pkgFiles": ["package.json"],
    "increment": "conventional:angular",
    "buildCommand": "npm start",
    "src": {
      "commitMessage": "Release v%s",
      "tagAnnotation": "Release v%s",
      "afterReleaseCommand": ""
    },
    "npm": {
      "publish": true,
      "tag": "latest"
    },
    "github": {
      "release": true,
      "releaseName": "v%s",
      "tokenRef": "GITHUB_TOKEN",
      "assets": "dist.zip"
    }
  }